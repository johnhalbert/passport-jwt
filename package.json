{
  "name": "passport-jwt",
  "version": "5.0.0",
  "description": "Passport authentication strategy using JSON Web Tokens",
  "scripts": {
    "test": "mocha --reporter spec --require test/bootstrap test/*test.js --timeout 10000",
    "testcov": "nyc npm run test",
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc -p config/tsconfig.cjs.json",
    "build:esm": "tsc -p config/tsconfig.esm.json",
    "build:types": "tsc -p config/tsconfig.types.json",
    "clean": "rimraf ./dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mikenicholson/passport-jwt.git"
  },
  "keywords": [
    "Passport",
    "Strategy",
    "JSON",
    "Web",
    "Token",
    "JWT"
  ],
  "author": "Mike Nicholson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikenicholson/passport-jwt/issues"
  },
  "homepage": "https://github.com/mikenicholson/passport-jwt",
  "devDependencies": {
    "chai": "^4.3.6",
    "chai-passport-strategy": "^3.0.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.16",
    "@types/passport": "^1.0.10",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.6.0",
    "tslib": "^2.4.0"
  },
  "peerDependencies": {
    "@nestjs/jwt": "^9.0.0 || ^8.0.0 || ^7.0.0",
    "jose": "^4.0.0 || ^3.0.0"
  },
  "peerDependenciesMeta": {
    "jose": {
      "optional": true
    },
    "@nestjs/jwt": {
      "optional": true
    }
  },
  "typesVersions": {
    "*": {
      "auto": ["./dist/types/auto/index.d.ts"]
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./auto": {
      "require": {
        "types": "./dist/types/auto/index.d.ts",
        "default": "./dist/cjs/auto/index.js"
      },
      "import": {
        "types": "./dist/types/auto/index.d.ts",
        "default": "./dist/esm/auto/index.js"
      }
    },
    "./platform-jose": {
      "require": {
        "types": "./dist/types/platforms/jose.d.ts",
        "default": "./dist/cjs/platforms/jose.js"
      },
      "import": {
        "types": "./dist/types/platforms/jose.d.ts",
        "default": "./dist/esm/platforms/jose.js"
      }
    },
    "./platform-jsonwebtoken": {
      "require": {
        "types": "./dist/types/platforms/jsonwebtoken.d.ts",
        "default": "./dist/cjs/platforms/jsonwebtoken.js"
      },
      "import": {
        "types": "./dist/types/platorms/jsonwebtoken.d.ts",
        "default": "./dist/esm/platforms/jsonwebtoken.js"
      }
    },
    "./platform-nestjsjwt": {
      "require": {
        "types": "./dist/types/platforms/nestjsjwt.d.ts",
        "default": "./dist/cjs/platforms/nestjsjwt.js"
      },
      "import": {
        "types": "./dist/types/platforms/nestjsjwt.d.ts",
        "default": "./dist/esm/platforms/nestjsjwt.js"
      }
    }
  },
  "types": "./fallback.d.ts"
}
