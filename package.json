{
  "name": "passport-jwt",
  "version": "5.0.0",
  "type": "module",
  "main": "./fallback.cjs",
  "types": "./fallback.d.ts",
  "description": "Passport authentication strategy using JSON Web Tokens",
  "engines": {
    "node": ">=12.19.0 || >=14.6.0 || >=16.0.0"
  },
  "scripts": {
    "test": "mocha --reporter spec --require test/bootstrap.cjs test/*test.cjs --timeout 10000",
    "testcov": "nyc npm run test",
    "build": "npm run build:prebuild && npm run build:cjs && npm run build:esm && npm run build:types && npm run build:postbuild",
    "build:prebuild": "rimraf ./dist",
    "build:cjs": "tsc -p config/tsconfig.cjs.json",
    "build:esm": "tsc -p config/tsconfig.esm.json",
    "build:types": "tsc -p config/tsconfig.types.json",
    "build:postbuild": "convert-extension cjs ./dist/cjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mikenicholson/passport-jwt.git"
  },
  "keywords": [
    "Passport",
    "Strategy",
    "JSON",
    "Web",
    "Token",
    "JWT"
  ],
  "author": "Mike Nicholson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikenicholson/passport-jwt/issues"
  },
  "homepage": "https://github.com/mikenicholson/passport-jwt",
  "files": [
    "dist",
    "fallback.cjs",
    "fallback.d.ts",
    "auto.d.ts",
    "platform-jose.d.ts",
    "platform-jsonwebtoken.d.ts",
    "platform-nestjsjwt.d.ts"
  ],
  "devDependencies": {
    "@nestjs/jwt": "^9.0.0",
    "chai": "^4.3.6",
    "chai-passport-strategy": "^3.0.0",
    "convert-extension": "^0.3.0",
    "jose": "^4.0.0",
    "jsonwebtoken": "^8.0.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.16",
    "@types/passport": "^1.0.10",
    "passport": "^0.6.0",
    "tslib": "^2.4.0"
  },
  "peerDependencies": {
    "@nestjs/jwt": "^9.0.0 || ^8.0.0 || ^7.0.0",
    "jose": "^4.0.0 || ^3.0.0",
    "jsonwebtoken": "^8.0.0"
  },
  "peerDependenciesMeta": {
    "jose": {
      "optional": true
    },
    "@nestjs/jwt": {
      "optional": true
    },
    "jsonwebtoken": {
      "optional": true
    }
  },
  "typesVersions": {
    "<4.7": {
      ".": [
        "./dist/types/index.d.ts"
      ],
      "./auto": [
        "./dist/types/auto_load.d.ts"
      ],
      "./platform-nestjsjwt": [
        "./dist/types/platforms/nestjsjwt.d.ts"
      ],
      "./platform-jose": [
        "./dist/types/platforms/jose.d.ts"
      ],
      "./platform-jsonwebtoken": [
        "./dist/types/platforms/jsonwebtoken.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      },
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./auto": {
      "require": {
        "types": "./dist/types/auto_load.d.ts",
        "default": "./dist/cjs/auto_load.cjs"
      },
      "import": {
        "types": "./dist/types/auto_load.d.ts",
        "default": "./dist/esm/auto_load.js"
      }
    },
    "./platform-jose": {
      "require": {
        "types": "./dist/types/platforms/jose.d.ts",
        "default": "./dist/cjs/platforms/jose.cjs"
      },
      "import": {
        "types": "./dist/types/platforms/jose.d.ts",
        "default": "./dist/esm/platforms/jose.js"
      }
    },
    "./platform-jsonwebtoken": {
      "require": {
        "types": "./dist/types/platforms/jsonwebtoken.d.ts",
        "default": "./dist/cjs/platforms/jsonwebtoken.cjs"
      },
      "import": {
        "types": "./dist/types/platorms/jsonwebtoken.d.ts",
        "default": "./dist/esm/platforms/jsonwebtoken.js"
      }
    },
    "./platform-nestjsjwt": {
      "require": {
        "types": "./dist/types/platforms/nestjsjwt.d.ts",
        "default": "./dist/cjs/platforms/nestjsjwt.cjs"
      },
      "import": {
        "types": "./dist/types/platforms/nestjsjwt.d.ts",
        "default": "./dist/esm/platforms/nestjsjwt.js"
      }
    }
  }
}
